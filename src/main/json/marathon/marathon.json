{
	"container": {
		"type": "DOCKER",
		"docker": {
			"image": "docker-registry-hostname/triathlon:v0.0.7"
		}
	},
	"env": {
		"archaius.deployment.environment": "pro"
	},
	"id": "triathlon",
	"instances": 1,
	"ports": [8077, 8080],
	"requirePorts": true,
	"cpus": 0.5,
	"constraints": [["datacenter", "Alberts-MacBook-Pro.local"]],
	"mem": 512
}
